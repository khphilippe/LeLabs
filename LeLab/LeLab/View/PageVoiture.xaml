             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             BackgroundColor="#f4f4f4"
             Title="Voiture"
             IconImageSource="lab_icon_voiture.png"
             x:Class="LeLab.View.PageVoiture">
    <ContentPage.Content>
        
        <StackLayout  HorizontalOptions="FillAndExpand" VerticalOptions="Center" >
            <Image x:Name="voitureImage" Source="lab_voiture.png"  Margin="30,30" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>